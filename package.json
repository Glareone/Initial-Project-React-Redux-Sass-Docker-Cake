{
  "name": "Initial-Project-React-Redux-v.2",
  "version": "1.0.0",
  "description": "Single Page Application Structure",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "autoprefixer": "^9.4.4",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
